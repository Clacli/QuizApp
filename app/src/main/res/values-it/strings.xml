    <!-- Sub-header of the quiz. [CHAR LIMIT=NONE] -->
    <string name="quiz_subheader">Anche le piante hanno un nome e una famiglia! Metti alla prova le tue conoscenze</string>
    <!-- Description of the bilberry image. [CHAR LIMIT=NONE] -->
    <string name="bilberry_shrub">un arbusto di mirtillo</string>
    <!-- Text of the bilberry and myrtle question. [CHAR LIMIT=NONE] -->
    <string name="bilberry_question_text">Il mirtillo, <xliff:g example="Vaccinium myrtillus L." id="nameOfBilberry"><i>Vaccinium myrtillus</i> L.</xliff:g>, e il mirto, <xliff:g name="nameOfMyrtle" example="Myrtus communis L."><i>Myrtus communis</i> L.</xliff:g>, appartengono alla stessa famiglia?</string>
    <!-- Text of the affirmative option answer. [CHAR LIMIT=15] -->
    <string name="affirmative_answer">Si</string>
    <!-- Description of the fly and daisy image. [CHAR LIMIT=NONE] -->
    <string name="fly_on_a_daisy">una mosca su una margherita</string>
    <!-- Text of the negative option answer. [CHAR LIMIT=15] -->
    <string name="negative_answer">No</string>
    <!-- Text of the asteraceae question. [CHAR LIMIT=NONE] -->
    <string name="asteraceae_question">Quali tra queste piante appartiene alla famiglia delle <xliff:g example="Asteraceae" id="familyAsteraceae">Asteraceae</xliff:g>?</string>
    <!-- Text of the fennel option answer. [CHAR LIMIT=NONE] -->
    <string name="foeniculum_vulgare_fennel"><xliff:g name="nameOfFennel" example="Foeniculum vulgare Mill."><i>Foeniculum vulgare</i> Mill.</xliff:g> - finocchio</string>
    <!-- Text of the daisy option answer. [CHAR LIMIT=NONE] -->
    <string name="daisy_answer"><xliff:g name="nameOfDaisy" example="Bellis perennis L."><i>Bellis perennis</i> L.</xliff:g> - margherita</string>
    <!-- Description of the hoary plantain inflorescence image. [CHAR LIMIT=NONE] -->
    <string name="inflorescence_of_plantago_media">infiorescenza di <xliff:g name="hoaryPlantago" example="plantago media">plantago media</xliff:g></string>
    <!-- Text of the marygold option answer. [CHAR LIMIT=NONE] -->
    <string name="marigold_answer"><xliff:g name="nameOfMarygold" example="Calendula officinalis L."><i>Calendula officinalis</i> L.</xliff:g> - calendula</string>
    <!-- Text of the hoary plantain question. [CHAR LIMIT=NONE] -->
    <string name="plantago_question">La <xliff:g name="nameOfHoaryPlantain" example="Plantago media L."><i>Plantago media</i> L.</xliff:g>, piantaggine pelosa, appartiene alla famiglia delle <xliff:g example="Plantaginaceae" id="familyPlantaginaceae">Plantaginaceae</xliff:g>. Come si chiama la sua infiorescenza?</string>
    <!-- Text of the edit text hint. [CHAR LIMIT=20] -->
    <string name="answer_hint">Rispondi qui</string>
    <!-- Text of the answer to the hoary plantain question. [CHAR LIMIT=20] -->
    <string name="spike">Spiga</string>
    <!-- Text of the olea anf fraxinus sp. question. [CHAR LIMIT=NONE] -->
    <string name="olive_family_question">A che famiglia botanica appartengono l\'<xliff:g name="nameOfOliveTree" example="Olea europaea L."><i>Olea europaea</i> L.</xliff:g> (olivo), il <xliff:g name="nameOfEuropaeanAsh" example="Fraxinus excelsior L."><i>Fraxinus excelsior</i> L.</xliff:g> (frassino) e il <xliff:g name="nameOfMannaAsh" example="Fraxinus ornus L."><i>Fraxinus ornus</i> L.</xliff:g> (orniello)?</string>
    <!-- Text of the Oleaceae option answer. [CHAR LIMIT=8] -->
    <string name="oleaceae"><xliff:g example="Oleaceae" id="familyOleaceae">Oleaceae</xliff:g></string>
    <!-- Text of the Fraxinaceae option answer. [CHAR LIMIT=11] -->
    <string name="fraxinaceae"><xliff:g example="Fraxinaceae" id="fakeFamilyFraxinaceae">Fraxinaceae</xliff:g></string>
