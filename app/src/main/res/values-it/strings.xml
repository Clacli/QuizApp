<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Title for the application. [CHAR LIMIT=12] -->
    <string name="app_name"><xliff:g name="nameOfApplication" example="Quiz App">Quiz App</xliff:g></string>
    <!-- Title header of the quiz. [CHAR LIMIT=20] -->
    <string name="quiz_header">Quiz Botanico</string>
    <!-- Name of the submit button [CHAR LIMIT=20] -->
