    <!-- Description of the flower of the lemon tree image. [CHAR LIMIT=NONE] -->
    <string name="flowered_lemon_tree">formica su un fiore di limone</string>
    <!-- Text of the botanical genus of the lemon tree question. [CHAR LIMIT=NONE] -->
    <string name="citrus_question">L\'arancio, il limone, l\'albero del pompelmo e quello del lime appartengono tutti alla stessa famiglia (<xliff:g example="Rutaceae" id="familyRutaceae">Rutaceae</xliff:g>) e allo stesso genere botanico. Quale?</string>
    <!-- Text of the answer to the genus of lemon tree question. [CHAR LIMIT=6] -->
    <string name="citrus"><xliff:g name="genusCitrus" example="Citrus">Citrus</xliff:g></string>
    <!-- Description of the flowers of a yarrow top image. [CHAR LIMIT=NONE] -->
    <string name="flowered_yarrow">fiori di achillea</string>
    <!-- Text of the botanical family of the Achillea genus question. [CHAR LIMIT=NONE] -->
    <string name="yarrow_question">A quale famiglia botanica appartiene il genere <xliff:g name="genusAchillea" example="Achillea L."><i>Achillea</i> L.</xliff:g>, achillea?</string>
    <!-- Text of the Asteraceae option answer. [CHAR LIMIT=10] -->
    <string name="yarrow_answer"><xliff:g example="Asteraceae" id="familyAsteraceae">Asteraceae</xliff:g></string>
    <!-- Text of the Campanulaceae option answer. [CHAR LIMIT=15] -->
    <string name="campanulaceae_answer"><xliff:g example="Campanulaceae" id="familyCampanulaceae">Campanulaceae</xliff:g></string>
    <!-- Description of the flowers of wild carrot image. [CHAR LIMIT=NONE] -->
    <string name="flowered_carrot">fiori di carota</string>
    <!-- Text of the Apiaceae botanical family question. [CHAR LIMIT=NONE] -->
    <string name="wild_carrot_question">La carota selvatica, <xliff:g example="Daucus carota L" id="nameOfCarrot"><i>Daucus carota</i> L.</xliff:g>, appartiene alla famiglia delle <xliff:g example="Apiaceae" id="familyApiaceae">Apiaceae</xliff:g>. Quali delle seguenti piante appartiene alla stessa famiglia?</string>
    <!-- Text of the cumin option answer. [CHAR LIMIT=20] -->
    <string name="cuminum_answer"><xliff:g example="Cuminum cyminum L." id="nameOfCumin"><i>Cuminum cyminum</i> L.</xliff:g> - cumino</string>
    <!-- Text of the caraway option answer. [CHAR LIMIT=20] -->
    <string name="caraway_answer"><xliff:g example="Carum carvi L." id="nameOfCaraway"><i>Carum carvi</i> L.</xliff:g> - carvi</string>
    <!-- Text of the estragon option answer. [CHAR LIMIT=25] -->
    <string name="estragon_answer"><xliff:g example="Artemisia dracunculus L." id="nameOfEstragon"><i>Artemisia dracunculus</i> L.</xliff:g> - dragoncello</string>
    <!-- Text of the spice from Crocus question. [CHAR LIMIT=NONE] -->
    <string name="crocus_question">Il <xliff:g example="Crocus sativus L." id="nameOfSafftonHerb"><i>Crocus sativus</i> L.</xliff:g> (<xliff:g example="Iridaceae" id="familyIridaceae">Iridaceae</xliff:g>) viene coltivato da migliaia di anni per ottenere una spezia molto preziosa. Quale?</string>
    <!-- Description of the inflorescence of Lythrum salicaria image. [CHAR LIMIT=NONE] -->
    <string name="flowered_lythrum_salicaria">infliorescenza di <xliff:g example="Lythrum salicaria" id="salcerellaFlowered">Lythrum salicaria</xliff:g></string>
    <!-- Text of the question on one of Lythrum salicaria common name. [CHAR LIMIT=NONE] -->
    <string name="lythrum_question">Salcerella è uno dei nomi comuni del <xliff:g example="Lythrum salicaria L." id="nameOfPurpleLoosestrife"><i>Lythrum salicaria</i> L.</xliff:g> <xliff:g example="Lythraceae" id="familyLythraceae">, Lythraceae</xliff:g>?</string>
