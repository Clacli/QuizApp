    <string name="rose_hip">cinorrodo</string>
    <!-- Text of the Rosaceae botanical family question. [CHAR LIMIT=NONE] -->
    <string name="rosaceae_question">La rosa appartiene alla famiglia botanica delle <xliff:g example="Rosaceae" id="familyRosaceae">Rosaceae</xliff:g>. Quale o quali delle seguenti piante appartiene alla stessa famiglia?</string>
    <!-- Text of the apple tree option answer. [CHAR LIMIT=25] -->
    <string name="apple"><xliff:g name="nameOfAppleTree" example="Malus domestica Borkh."><i>Malus domestica</i> Borkh.</xliff:g> - melo</string>
    <!-- Text of the cornelian cherry tree option answer. [CHAR LIMIT=15] -->
    <string name="cornelian_cherry_answer"><xliff:g name="nameOfCornelianCherryTree" example="Cornus mas L."><i>Cornus mas</i> L.</xliff:g> - corniolo</string>
    <!-- Text of the hawthorn option answer. [CHAR LIMIT=25] -->
